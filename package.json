  "version": "1.4.1",
  "description": "Wallet for the Stellar payment network by Montelibero.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": {
    "name": "Montelibero Sunce Team",
    "email": "sunce@montelibero.org"
  },
  "homepage": "https://sunce.montelibero.org/",
